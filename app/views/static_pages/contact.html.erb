<div class="contact">
  <div class="container">
    <div class="contact-top heading">
      <h2><%= t ".contact_page" %></h2>
    </div>
    <div class="contact-text">
      <div class="col-md-6 contact-left">
        <div class="address">
          <h5><%= t ".C_address" %></h5>
          <p>
            <%= t ".C_name" %>
            <span><%= t ".Handico" %></span>
            <%= t ".C_Address" %>
          </p>
        </div>
        <div class="address">
          <h5><%= t ".C_address1" %></h5>
          <p>
            <%= t ".tel" %>
            <span><%= t ".fax" %></span>
            <p>
              <%= link_to t(".C_email"),"mailto:kensoloko@email.com" %>
            </p>
          </p>
        </div>
      </div>
      <div class="col-md-6 contact-right">
        <div class="map">
          <iframe src="https://goo.gl/RELPnw"
          frameborder="0" style="border:0" ></iframe>
        </div>
      </div>
      <div class="clearfix"></div>
    </div>
  </div>
</div>

